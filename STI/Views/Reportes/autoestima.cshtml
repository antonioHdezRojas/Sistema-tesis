
@{
    ViewBag.Title = "autoestima";
}

<h2>autoestima</h2>
@Html.Partial("navBar")
<legend>Autoestima</legend>
<form role="form">    
    <div class="form-group">
        <label for="txtGeneral">Calificacion total @ViewBag.general</label>       
        <input type="text" class="form-control" id="txtGeneral" disabled value="@ViewBag.mensaje">
    </div>
    <div class="form-group">
        <label for="txtCatGeneral">Autoestima General @ViewBag.catGeneral</label>
        <input type="text" class="form-control" id="txtCatGeneral" disabled value="@ViewBag.mensajeCatG">
    </div>
    <div class="form-group">
        <label for="txtCatFamilia">Autoestima en la Familia @ViewBag.catFamilia</label>
        <input type="text" class="form-control" id="txtCatFamilia" disabled value="@ViewBag.mensajeCatF">
    </div>
    <div class="form-group">
        <label for="txtCatEscolar">Autoestima en la escuela @ViewBag.catEscolar</label>
        <input type="text" class="form-control" id="txtCatEscolar" disabled value="@ViewBag.mensajeCatE">
    </div>
</form>
@if((int?)ViewBag.catGeneral<20 || (int?)ViewBag.catFamilia < 17 || (int?)ViewBag.catEscolar < 20)
{
    <a href = "@Url.Action("autoestima","Materiales")" >Material</a >
}
